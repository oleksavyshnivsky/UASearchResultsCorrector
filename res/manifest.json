{
	"manifest_version": 2,
	"name": "UA Google Results Corrector",
	"version": "0.1",
	"browser_action": {

	},
	"content_scripts": [
		{
			"matches": [
				"https://*.google.com/*",
				"https://google.com/*"
			],
			"js": ["content.js"]
		}
	],
	"icons": {
		"16": "icon16.png",
		"48": "icon48.png",
		"128": "icon128.png"
	}
}